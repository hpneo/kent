<h1>Importing my feeds</h1>

<%= form_tag('/feeds/import_subscriptions', multipart: true, method: 'post') do %>
  <p>
    <%= label_tag 'import[subscriptions]', 'Choose an OPML file' %>
    <%= file_field_tag 'import[subscriptions]' %>
  </p>
  <%= submit_tag 'Import', class: 'button green' %>
<% end %>
